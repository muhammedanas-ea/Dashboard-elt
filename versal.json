{
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/.*",
        "dest": "http://localhost:8000/$1"
      },
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ]
  }
  